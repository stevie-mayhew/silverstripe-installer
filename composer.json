{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.5.0",
		"silverstripe/cms": "dev-master",
		"silverstripe/framework": "dev-master",
		"silverstripe/reports": "dev-master",
		"silverstripe/siteconfig": "dev-master",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/asset-admin": "^1"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~4.8"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.0.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"SilverStripe\\Installer\\": "src/SilverStripe/Installer/"
		}
	},
	"scripts": {
		"post-create-project-cmd": "SilverStripe\\Installer\\PostCreateProjectCommand::process"
	}
}
